 public class Kad02_1{
	public static void main(String[]args){
		System.out.println("10+2="+(10+2));
        	System.out.println("10-2="+(10-2));
		System.out.println("10*10="+(10*10));
		System.out.println("10.5+20.3="+(10.5+20.3));
		System.out.println("-9%5+-9%-5="+(-9%5+-9%-5));
	}
 }
